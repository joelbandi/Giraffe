<div class="nav">
  <h3 class="nav__title">Quick Start</h3>
  <ul class="nav__menu">
    <li class="nav__menu-item<%= path === '/' ? ' nav__menu-item--selected' : '' %>">
      <a class="nav__link" href="<%= base%>/">Overview</a>
    </li>
    <% const navTabs = [
      {file:'getting-started',tabName: 'Getting Started'},
      {file:'api-methods',tabName: 'API Methods'},
      {file:'examples',tabName: 'Examples'},
    ] %>
    <% navTabs.map(function(tab) { %>
      <li class="nav__menu-item<%= path === ('/'+tab.file) ? ' nav__menu-item--selected' : '' %>">
        <a class="nav__link" href="<%= base%>/<%= tab.file%>"><%= tab.tabName%></a>
      </li>
    <% })%>

  </ul>
</div>
