{
  "name": "giraffy",
  "version": "0.0.5",
  "description": "An Imgur client/wrapper",
  "repository": "joelbandi/giraffy",
  "author": "joelbandi",
  "license": "MIT",
  "keywords": [],
  "main": "index.js",
  "jsnext:main": "index.es6.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base",
    "rules": {
      "func-names": 0,
      "no-restricted-syntax": 0,
      "prefer-const": 0
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babel-runtime": "^6.6.1",
    "urijs": "^1.18.10"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.1",
    "bs-html-injector": "^3.0.3",
    "chai": "^4.0.1",
    "chai-http": "^3.0.0",
    "coveralls": "^2.13.1",
    "del": "^2.2.0",
    "easystatic": "^0.1.12",
    "esdoc": "^0.5.2",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^3.4.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "sinon": "^2.0.0-pre"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "publish": "npm run build && cd dist && npm publish && cd ..",
    "publish:docs": "easystatic deploy docs --repo joelbandi/giraffy",
    "start:docs": "easystatic start docs",
    "deploy:docs": "easystatic deploy docs --repo=joelbandi/giraffy",
    "view:coveralls": "open coverage/lcov-report/index.html"
  }
}
